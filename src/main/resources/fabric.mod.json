{
  "schemaVersion": 1,
  "id": "cartses",
  "version": "${version}",
  "name": "Cartses",
  "description": "More minecart types",
  "authors": [
    "BoogieMonster1O1"
  ],
  "contact": {},
  "license": "ISC",
  "icon": "assets/cartses/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "io.github.boogiemonster1o1.cartses.CartsesInitializer",
        "adapter": "scala"
      }
    ],
    "mm:early_risers": [
      {
        "value": "io.github.boogiemonster1o1.cartses.CartsesEarlyRiser",
        "adapter": "scala"
      }
    ],
    "client": [
      {
        "value": "io.github.boogiemonster1o1.cartses.client.CartsesClient",
        "adapter": "scala"
      }
    ],
    "dynamiclights": [
      {
        "value": "io.github.boogiemonster1o1.cartses.client.ldl.LdlInitializer",
        "adapter": "scala"
      }
    ],
    "server": []
  },
  "mixins": [
    "cartses.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "recommends": {
    "lambdynlights": "*"
  }
}
